<?xml version="1.0" encoding="UTF-8" ?>
	<widget xmlns		= "http://www.w3.org/ns/widgets"
			xmlns:gap	= "http://phonegap.com/ns/1.0"
			id			= "mx.italiana.ItaExtiV1"
			versionCode	= "1"
			version		= "1.0.0" >
		<name>
			ItaExtiV1
		</name>
		<description>
			Grupo Industrial La Italiana
		</description>
		<author href="www.laitaliana.com.mx" email="luis_martinez@laitaliana.com.mx">
			Pinky
		</author>
		<preference name="phonegap-version" value="cli-6.5.0" />
		  
        <icon src="icon.png" />
		

		<preference name="orientation" value="portrait"></preference>		
		<!--<gap:plugin name="org.apache.cordova.dialogs"></gap:plugin>-->
		<!--<gap:plugin name="org.apache.cordova.vibration"></gap:plugin>-->
		<!--<gap:plugin name="org.apache.cordova.device"></gap:plugin>-->
		<!--<gap:plugin name="com.phonegap.plugins.barcodescanner"></gap:plugin>-->
		<!--<gap:plugin name="org.apache.cordova.network-information" />-->

<plugin name="cordova-plugin-device" spec="2.0.1" />
<plugin name="cordova-plugin-dialogs" spec="2.0.1" />
<plugin name="cordova-plugin-network-information" spec="2.0.1" />
<plugin name="cordova-plugin-vibration" spec="3.0.1" />
<plugin name="cordova-plugin-whitelist" spec="1.3.3" />
<plugin name="phonegap-plugin-barcodescanner" spec="7.0.2" />

<config-file target="AndroidManifest.xml" parent="/*" mode="merge">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
</config-file>

		<access origin="*" />
        <allow-intent href="*" />
        <allow-navigation href="*" />
	</widget>